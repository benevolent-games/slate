{
	"compilerOptions": {
		"strict": true,
		"rootDir": "s",
		"outDir": "x",
		"target": "es2023",
		"lib": ["es2023", "dom"],
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"declaration": true,
		"skipLibCheck": true
	},
	"include": [
		"s/**/*.ts",
		"s/**/*.js"
	],
	"exclude": [
		"s/**/_*"
	]
}
